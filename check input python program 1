ch = input("Please Enter the input : ")

if(ch >= 'a' and ch <= 'z'):
    print("the given input is lower case and" , ch , "is a alphabet")
elif (ch >= 'A' and ch <= 'Z'): 
    print("The Given input is upper case and", ch, "is an Alphabet") 
elif(ch >= '0' and ch <= '9'):
    print("The Given input ", ch, "is a Digit")
